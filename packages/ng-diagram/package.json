{
  "name": "ng-diagram",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/ng-diagram/fesm2022/ng-diagram.mjs",
      "types": "./dist/ng-diagram/index.d.ts"
    },
    "./styles.css": "./dist/ng-diagram/styles.css",
    "./primitives.css": "./dist/ng-diagram/primitives.css",
    "./tokens.css": "./dist/ng-diagram/tokens.css"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration development",
    "build:prod": "ng build && node scripts/remove-sourcemaps.js && node scripts/minify-mjs.js && node scripts/bundle-dts.js && node scripts/fix-package-json.js",
    "publish:local": "cd dist/ng-diagram && npm publish --registry http://localhost:4873/",
    "prepublish": "pnpm build:prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@angular/common": "^19.2.7",
    "@angular/compiler": "^19.2.7",
    "@angular/core": "^20.3.4",
    "@angular/forms": "^19.2.10",
    "@angular/platform-browser": "^19.2.7",
    "@angular/platform-browser-dynamic": "^19.2.10",
    "@angular/router": "^19.2.7",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.16.0",
    "@analogjs/vite-plugin-angular": "^1.15.1",
    "@analogjs/vitest-angular": "^1.19.1",
    "@angular-devkit/build-angular": "^19.2.8",
    "@angular-eslint/builder": "^19.3.0",
    "@angular/cli": "^19.2.8",
    "@angular/compiler-cli": "^19.2.7",
    "@eslint/js": "^9.25.1",
    "@microsoft/api-extractor": "^7.53.1",
    "@nx/vite": "^20.8.0",
    "angular-eslint": "19.3.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jsdom": "^26.1.0",
    "ng-packagr": "^19.2.2",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
