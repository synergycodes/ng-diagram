{
  "name": "ng-diagram",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/ng-diagram/fesm2022/ng-diagram.mjs",
      "types": "./dist/ng-diagram/index.d.ts"
    },
    "./styles.css": "./dist/ng-diagram/styles.css",
    "./primitives.css": "./dist/ng-diagram/primitives.css",
    "./tokens.css": "./dist/ng-diagram/tokens.css"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && node scripts/fix-package-json.js",
    "publish:local": "cd dist/ng-diagram && npm publish --registry http://localhost:4873/",
    "prepublish": "pnpm build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.16.0",
    "@analogjs/vite-plugin-angular": "^1.15.1",
    "@analogjs/vitest-angular": "^1.19.1",
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular-eslint/builder": "^18.0.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@eslint/js": "^9.25.1",
    "@nx/vite": "^20.8.0",
    "angular-eslint": "18.0.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jsdom": "^26.1.0",
    "ng-packagr": "^18.1.0",
    "typescript": "~5.4.0",
    "typescript-eslint": "8.31.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
